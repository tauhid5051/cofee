	<queryString>
		<![CDATA[SELECT   DATE(sma_payments.Date) Date,SUM(amount) PathologyPaid
,0 ServicePaid,SUM(Amount) TotalAmount
FROM sma_sales,sma_payments,sma_users
WHERE sma_sales.id=sma_payments.sale_id AND sma_payments.created_by=sma_users.id
AND DATE(sma_payments.Date) BETWEEN $P{startDate} AND  $P{endDate}
AND sma_payments.created_by =(CASE WHEN $P{createdBy}='all' THEN sma_users.id ELSE $P{createdBy} END)
GROUP BY  DATE(sma_payments.Date)]]>
	</queryString>
